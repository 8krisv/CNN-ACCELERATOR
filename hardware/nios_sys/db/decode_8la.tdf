	eq[] = eq_node[];
	eq_node[3..0] = eq_wire[3..0];
	eq_wire[] = ( w_anode2178w[2..2], w_anode2170w[2..2], w_anode2162w[2..2], w_anode2149w[2..2]);
	w_anode2149w[] = ( (w_anode2149w[1..1] & (! data_wire[1..1])), (w_anode2149w[0..0] & (! data_wire[0..0])), enable_wire);
