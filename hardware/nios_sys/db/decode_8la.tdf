	w_anode2170w[] = ( (w_anode2170w[1..1] & data_wire[1..1]), (w_anode2170w[0..0] & (! data_wire[0..0])), enable_wire);
	w_anode2178w[] = ( (w_anode2178w[1..1] & data_wire[1..1]), (w_anode2178w[0..0] & data_wire[0..0]), enable_wire);
